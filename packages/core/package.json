{
  "name": "@pando/core",
  "version": "0.3.2",
  "description": "Core contracts for pando",
  "author": "Ryhope Network <contact@ryhope.network>",
  "contributors": [
    "Olivier Sarrouy <olivier@ryhope.network>"
  ],
  "license": "(GPL-3.0-or-later OR MIT)",
  "repository": "github:ryhope/pando",
  "keywords": [
    "vcs",
    "git",
    "pando",
    "ipfs",
    "ipld",
    "ethereum",
    "aragon",
    "ryhope"
  ],
  "scripts": {
    "compile": "./node_modules/.bin/truffle compile",
    "migrate": "./node_modules/.bin/truffle migrate",
    "test": "TRUFFLE_TEST=true npm run ganache-cli:test",
    "lint": "./node_modules/.bin/solium --dir ./contracts --fix",
    "coverage": "SOLIDITY_COVERAGE=true npm run ganache-cli:test",
    "console": "./node_modules/.bin/truffle dev",
    "ganache-cli:test": "scripts/ganache-cli.sh"
  },
  "files": [
    "build/",
    "contracts/",
    "migrations/",
    "scripts/",
    "truffle.js",
    "test/"
  ],
  "dependencies": {},
  "devDependencies": {
    "@aragon/apps-shared-minime": "^1.0.0",
    "@aragon/apps-token-manager": "^1.0.0",
    "@aragon/os": "^4.0.0-beta.3",
    "@aragon/test-helpers": "^1.0.1",
    "@ryhope/truffle": "^5.0.0-beta.1",
    "solidity-coverage": "^0.5.8",
    "solium": "^1.1.8",
    "web3-utils": "^1.0.0-beta.36"
  }
}
