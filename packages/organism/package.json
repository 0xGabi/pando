{
  "name": "@pando/organism",
  "version": "0.0.1",
  "description": "Core contracts for pando",
  "author": "Pando Network <contact@pando.network>",
  "contributors": [
    "Olivier Sarrouy <olivier@pando.network>"
  ],
  "repository": "github:pandonetwork/pando",
  "homepage": "https://github.com/pandonetwork/pando",
  "bugs": "https://github.com/pandonetwork/pando/issues",
  "license": "(GPL-3.0-or-later OR MIT)",
  "keywords": [
    "pando",
    "vcs",
    "dao",
    "ipfs",
    "ethereum",
    "aragon"
  ],
  "scripts": {
    "build": "truffle compile",
    "migrate": "truffle migrate",
    "test": "aragon contracts test test/all.spec.js --network devnet",
    "lint": "./node_modules/.bin/solium --dir ./contracts --fix",
    "coverage": "SOLIDITY_COVERAGE=true npm run ganache-cli:test",
    "console": "./node_modules/.bin/truffle dev",
    "ganache-cli:test": "scripts/ganache-cli.sh",
    "publish:devnet:organism": "cd apps/organism && aragon apm publish 0.0.1 --environment default",
    "publish:devnet:lineage": "cd apps/lineage && aragon apm publish 0.0.1 --environment default",
    "publish:devnet:genesis": "cd apps/genesis && aragon apm publish 0.0.1 --environment default",
    "publish:devnet": "npm run publish:devnet:lineage && npm run publish:devnet:genesis && npm run publish:devnet:organism",
    "test:organism": "cd organism && truffle test --network devnet",
    "build:kit": "cd colony/app && npm run build:kit && cd ../.. && npm run run:kit",
    "run:kit": "aragon run --kit Kit --kit-init @ARAGON_ENS --files app/public",
    "start": "aragon run",
    "compile": "aragon contracts compile",
    "publish": "aragon publish",
    "dev:reset": "cd ../.. && npm run dev:reset"
  },
  "files": [
    "build/",
    "contracts/",
    "migrations/",
    "scripts/",
    "truffle.js",
    "test/"
  ],
  "dependencies": {
    "@aragon/os": "^4.0.0-beta.3"
  },
  "devDependencies": {
    "@aragon/cli": "5.2.0",
    "@aragon/test-helpers": "^1.0.1",
    "solidity-coverage": "^0.5.8",
    "solium": "^1.1.8",
    "truffle": "5.0.0-beta.2",
    "truffle-typings": "^1.0.5",
    "typechain": "^0.3.9"
  }
}
