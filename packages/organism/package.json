{
  "name": "@pando/organism",
  "version": "0.0.1",
  "description": "@aragon apps for @pando's organisms",
  "author": "Pando Network <contact@pando.network>",
  "contributors": [
    "Olivier Sarrouy <olivier@pando.network>"
  ],
  "repository": "github:pandonetwork/pando",
  "homepage": "https://github.com/pandonetwork/pando",
  "bugs": "https://github.com/pandonetwork/pando/issues",
  "license": "(GPL-3.0-or-later OR MIT)",
  "keywords": [
    "pando",
    "vcs",
    "dao",
    "ipfs",
    "ethereum",
    "aragon"
  ],
  "scripts": {
    "compile": "aragon contracts compile",
    "test:organism": "aragon contracts test test/organism.spec.js --network devnet",
    "test": "npm run test:organism",
    "publish:devnet:lineage": "cd apps/lineage && aragon apm publish patch --environment default",
    "publish:devnet:genesis": "cd apps/genesis && aragon apm publish patch --environment default",
    "publish:devnet:organism": "cd apps/organism && aragon apm publish patch --environment default",
    "publish:devnet": "npm run publish:devnet:lineage && npm run publish:devnet:genesis && npm run publish:devnet:organism",
    "publish": "npm run publish:devnet"
  },
  "dependencies": {
    "@aragon/apps-shared-minime": "^1.0.0",
    "@aragon/os": "4.0.1",
    "@pando/core": "^0.0.1"
  },
  "devDependencies": {
    "@aragon/test-helpers": "^1.0.1",
    "@pando/helpers": "^0.0.1",
    "truffle": "5.0.0-beta.2"
  }
}
